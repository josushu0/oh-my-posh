{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#eba0ac",
					"style": "plain",
					"template": "@{{ .UserName }} ",
					"type": "session"
				},
				{
					"foreground": "#94e2d5",
					"style": "plain",
					"template": "\u279c ",
					"type": "text"
				},
				{
					"foreground": "#f2cdcd",
					"properties": {
						"style": "folder"
					},
					"style": "plain",
					"template": " {{ .Path }}",
					"type": "path"
				},
				{
					"foreground": "#b4befe",
					"properties": {
						"branch_icon": ""
					},
					"style": "plain",
					"template": " <#fab387>git(</>{{ .HEAD }}<#fab387>)</>",
					"type": "git"
				},
				{
					"foreground": "#f38ba8",
					"style": "plain",
					"template": " \u2718",
					"type": "exit"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
